<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyTheme_Setting" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="actionBarStyle">@style/MyTheme_Setting.ActionBarStyle</item>
        <item name="android:actionBarStyle">@style/MyTheme_Setting.ActionBarStyle</item>
        <item name="android:titleTextStyle">@style/MyTheme_Setting.TitleTextStyle</item>
    </style>

    <style name="MyTheme_Setting.ActionBarStyle" parent="@style/Widget.AppCompat.ActionBar">
        <item name="background">@color/color_00cccc</item>
        <item name="android:background">@color/color_00cccc</item>
    </style>

    <style name="MyTheme_Setting.TitleTextStyle">
        <item name="android:textColor">@color/color_ffffff</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="MyTheme_Quelist" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/color_white</item>
    </style>

    <style name="Question_TextView_Style">
        <item name="android:shadowColor">@color/color_ffff99</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>
</resources>