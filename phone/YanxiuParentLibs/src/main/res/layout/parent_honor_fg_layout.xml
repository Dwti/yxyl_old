<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/honor_bg"
    android:orientation="vertical">
    <include android:id="@+id/top_layout"
        layout="@layout/public_parent_top_layout"
        />
    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/dimen_p_25"
        android:layout_marginRight="@dimen/dimen_p_25"
        android:id="@+id/honorList">
    </android.support.v7.widget.RecyclerView>
</LinearLayout>
