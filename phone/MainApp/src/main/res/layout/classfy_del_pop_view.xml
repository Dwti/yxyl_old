<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/container"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_margin="@dimen/dimen_50"
              android:background="@drawable/login_and_pwd_selector">

    <TextView
          android:layout_marginLeft="@dimen/dimen_15"
          android:layout_marginRight="@dimen/dimen_15"
          android:layout_width="match_parent"
          android:layout_height="@dimen/dimen_41"
          android:id="@+id/classfyDelPopText"
          android:gravity="center"
          android:textColor="@color/color_333333"
          android:textSize="@dimen/dimen_17"
        />


    <com.yanxiu.gphone.student.view.question.classfy.ClassfyAnswers
        android:id="@+id/classfy_text_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:visibility="gone"/>
    <com.common.core.view.UnMoveGridView
        android:id="@+id/classfy_icon_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dimen_10"
        android:numColumns="4"
        android:visibility="gone"/>

</LinearLayout>




