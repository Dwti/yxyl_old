<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <include android:id="@+id/top_layout"
             layout="@layout/public_top_layout"
            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/yanxiu_my_frgment_bg"
        android:orientation="vertical"
        android:paddingTop="@dimen/dimen_25"
        android:paddingRight="@dimen/dimen_30"
        android:paddingLeft="@dimen/dimen_30">
        <include android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_48"
                 android:id="@+id/male_layout"
                 layout="@layout/activity_info_select_public_item"
            />
        <include
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_13"
            android:layout_marginRight="@dimen/dimen_13"
            layout="@layout/bitmap_repeat_layout"
            />
        <include android:layout_width="match_parent"
                 android:layout_height="@dimen/dimen_48"
                 android:id="@+id/female_layout"
                 layout="@layout/activity_info_select_public_item"
            />


    </LinearLayout>






</LinearLayout>