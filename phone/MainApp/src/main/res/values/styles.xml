<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="NavigationBarHide" parent="Theme.AppCompat.Light.DarkActionBar" />

    <style name="GridPasswordView" />

    <style name="SwitchTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Active thumb color & Active track color(30% transparency) -->
        <item name="colorControlActivated">@color/color_118989</item>
        <!-- Inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/color_99937a</item>
        <!-- Inactive track color(30% transparency) -->
        <item name="android:colorForeground">@color/color_99937a</item>
    </style>

    <style name="GridPasswordView.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>

        <item name="android:gravity">center</item>
        <item name="android:shadowColor">@color/color_a37a00</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
    </style>
<!--   <item name="android:background">@drawable/group_input_bg</item> -->
    <style name="GridPasswordView.EditText" parent="GridPasswordView.TextView">
        <item name="android:cursorVisible">false</item>
    </style>

    <style name="GridPasswordView.Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:layout_marginBottom">1dp</item>
    </style>
    <style name="radio_btn_style">
